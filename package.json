{
  "name": "discord-league",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:server && yarn build:client",
    "build:clean": "rimraf dist",
    "build:client": "cd client && yarn install && yarn build && cd .. && cp -r client/build dist/public",
    "build:server": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "migrate:latest": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "start:all:dev": "concurrently \"yarn start:server:dev\" \"cd client && yarn start\"",
    "start:server:dev": "cross-env NODE_ENV=development sh bin/watch.sh",
    "start": "NODE_ENV=production yarn migrate:latest && node dist/index.js",
    "test": "yarn lint && yarn test:unit && yarn build",
    "test:unit": "yarn build:clean && ava",
    "test:unit:watch": "ava --watch"
  },
  "engines": {
    "node": "^14.15.1"
  },
  "volta": {
    "node": "14.17.6"
  },
  "license": "MIT",
  "dependencies": {
    "@dl/api": "file:./apiTypings",
    "@sentry/node": "^6.2.2",
    "@types/chance": "^1.0.9",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "14.14.45",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.8",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "body-parser": "^1.19.0",
    "chance": "^1.1.4",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "discord.js": "^12.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "fp-ts": "^2.6.7",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.95.2",
    "mappersmith": "^2.31.2",
    "passport": "^0.5.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.5.1",
    "prettier": "^2.4.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "^3.5.1",
    "ava-fast-check": "^1.1.2",
    "dotenv": "^10.0.0",
    "fast-check": "^1.23.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
