  "main": "index.js",
  "scripts": {
    "build:client": "cd client && yarn install && yarn build && cd .. && cp -R client/build public",
    "build:server": "tsc",
    "build": "yarn build:client && yarn build:server",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start:all:dev:win": "concurrently \"yarn start:server:dev:win\" \"cd client && yarn start\"",
    "start:all:dev": "concurrently \"yarn start:server:dev\" \"cd client && yarn start\"",
    "start:server:dev:win": "SET NODE_ENV=development && sh bin/watch.sh",
    "start:server:dev": "NODE_ENV=development sh bin/watch.sh",
    "start": "node dist/index.js",
    "test": "yarn lint"
  },
