{
  "name": "discord-league",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:server && yarn build:client",
    "build:clean": "rm -rf dist",
    "build:client": "cd client && yarn install && yarn build && cd .. && cp -r client/build dist/public",
    "build:server": "tsc",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "migrate:latest": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "start:all:dev": "concurrently \"yarn start:server:dev\" \"cd client && yarn start\"",
    "start:server:dev": "cross-env NODE_ENV=development sh bin/watch.sh",
    "start": "NODE_ENV=production yarn migrate:latest && node dist/index.js",
    "test": "yarn lint && yarn test:unit && yarn build",
    "test:unit": "yarn build:clean && ava",
    "test:unit:watch": "ava --watch"
  },
  "engines": {
    "node": "^14.15.1"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/chance": "^1.0.9",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.11",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/node": "^14.14.12",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "already": "^1.12.0",
    "ava": "^3.5.1",
    "ava-fast-check": "^1.1.2",
    "body-parser": "^1.19.0",
    "chance": "^1.1.4",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "fast-check": "^1.23.0",
    "fp-ts": "^2.6.7",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "mappersmith": "^2.31.2",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "pg": "^7.17.1",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^4.1.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
